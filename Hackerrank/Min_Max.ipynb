    "import numpy as np\n",
    "N, M = [int(x) for x in input().split()]\n",
    "\n",
    "lst = []\n",
    "for i in range(N):\n",
    "    nums = list(map(int,input().split()))\n",
    "    lst.append(nums)\n",
    "\n",
    "arr = np.array(lst)\n",
    "min = arr.min(axis=1)\n",
    "\n",
    "max = min.max()\n",
    "print(max)"
   ]
  }
